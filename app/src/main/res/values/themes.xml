<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.PdfToImage" parent="Theme.MaterialComponents.DayNight.NoActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/indigo_300</item>
        <item name="colorPrimaryVariant">@color/indigo_300</item>
        <item name="colorOnPrimary">@color/white</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/indigo_300</item>
        <item name="colorSecondaryVariant">@color/indigo_300</item>
        <item name="colorOnSecondary">@color/black</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="l">?attr/colorPrimaryVariant</item>
        <!-- Customize your theme here. -->
        <item name="android:navigationBarColor">@color/indigo_300</item>
    </style>

    <style name="MySeekBar.Discrete" parent="Widget.AppCompat.SeekBar.Discrete">
        <item name="tickMarkTint">@color/indigo_300</item>
    </style>

    <style name="Theme.App.Starting" parent="Theme.SplashScreen">
        <item name="windowSplashScreenBackground">@color/indigo_300</item>
        <item name="windowSplashScreenAnimatedIcon">@drawable/ic_launcher_foreground</item>
        <item name="postSplashScreenTheme">@style/Theme.PdfToImage</item>
    </style>
</resources>
